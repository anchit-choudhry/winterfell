{
  "name": "@winterfell/ui",
  "version": "0.1.0",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "flow:server": "flow",
    "babel:compile": "NODE_ENV=development BABEL_ENV=development babel ./src -d ./lib",
    "lint:css": "stylelint \"./src/**/*.{css,js}\"",
    "lint:js": "NODE_ENV=production BABEL_ENV=production eslint ./src",
    "lint:fix": "yarn lint:js --fix && yarn lint:css --fix",
    "prettier:fix": "prettier --config .prettierrc.js ./src --write",
    "start": "node scripts/start",
    "build": "node scripts/build",
    "test": "node scripts/test",
    "test:ci": "CI=true yarn test",
    "test:coverage": "yarn test --coverage",
    "test:ci-coverage": "CI=true yarn test:coverage",
    "run:all": "yarn babel:compile && yarn lint:fix && yarn prettier:fix && yarn test:ci-coverage && yarn build"
  },
  "dependencies": {
    "@babel/core": "^7.21.5",
    "@babel/plugin-syntax-flow": "^7.21.4",
    "@babel/plugin-transform-react-jsx": "^7.21.5",
    "@babel/preset-flow": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.21.5",
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@reduxjs/toolkit": "^1.9.5",
    "@svgr/webpack": "^7.0.0",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "browserslist": "^4.21.5",
    "camelcase": "^7.0.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.1",
    "html-webpack-plugin": "^5.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-resolve": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.23",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.3.0",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^8.3.2",
    "prettier": "^2.8.8",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.11.0",
    "resolve": "^1.22.2",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^13.2.2",
    "semver": "^7.5.0",
    "source-map-loader": "^4.0.1",
    "tailwindcss": "^3.3.2",
    "terser-webpack-plugin": "^5.3.7",
    "web-vitals": "^3.3.1",
    "webpack": "^5.81.0",
    "webpack-manifest-plugin": "^5.0.0",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "flow-bin": "^0.205.0",
    "postcss-styled-syntax": "^0.4.0",
    "source-map-explorer": "^2.5.3",
    "style-loader": "^3.3.2",
    "stylelint": "^15.6.0",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^5.0.4",
    "webpack-dev-server": "^4.13.3"
  },
  "resolutions": {},
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended",
      "plugin:import/recommended",
      "prettier"
    ],
    "plugins": [
      "react",
      "prettier"
    ],
    "rules": {
      "sort-imports": [
        "error",
        {
          "ignoreDeclarationSort": true
        }
      ],
      "import/order": [
        "error",
        {
          "groups": [
            "external",
            "builtin",
            "internal",
            "sibling",
            "parent",
            "index"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "@babel/preset-flow",
      "@babel/preset-react",
      "react-app"
    ]
  }
}
